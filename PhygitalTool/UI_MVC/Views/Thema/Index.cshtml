@using Phygital.Domain.Questionsprocess;
@model IEnumerable<Flow>

@{
    ViewBag.Title = "Phygital";
    Layout = "_Layout";
}
<div name="outerdiv" class="container-fluid">
    <div class="text-center">
        <div class="row justify-content-center">
            <div class="col-8">
                <h5 class="mb-3">Themas</h5>
                <table class="sttable table table-striped ">
                    <thead class="sticky-top bg-white">
                    <tr>
                        <th class="col-4">Thema</th>
                        <th class="col-4">Wijzig</th>
                        <th class="col-4">Verwijder</th>
                        
                    </tr>
                    </thead>
                    <tbody id="SubthemaTable" class="">


                    </tbody>
                </table>
                <div class="row align-content-center">
                    <div class="col d-flex justify-content-end">
                        <a type="button" class="btn btn-warning" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </div>
                    <div class="col d-flex justify-content-start">
                        <a type="button" class="btn btn-warning" asp-area="" asp-controller="Thema" asp-action="Add">Thema aanmaken</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts
{
<script src="~/js/Thema/themaList.js" defer></script>
}